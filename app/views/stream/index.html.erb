<% content_for :templates do %>
<template class="message-template">
<%= render partial: 'message' %>
</template>
<% end %>

<section class="ui video">
  <div class="embed">
    <iframe width="1280" height="720" src="//www.youtube.com/embed/_7gm9aWHBbo?autoplay=1&controls=2&showinfo=0&rel=0" frameborder="0" allowfullscreen></iframe>
  </div>

  <div class="brand">
    <img src="http://placehold.it/600x115/f00" class="video-image">
    <img src="http://placehold.it/600x280/0ff" class="timeline-image">
  </div>
</section>

<section class="ui timeline">
  <article class="collection messages">
    <% for i in 1..3 do %>
    <%= render partial: 'message' %>
    <% end %>
  </article>
</section>